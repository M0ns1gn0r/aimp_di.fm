    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DI.FM.WPF;assembly=DI.FM.WPF"
    xmlns:fsxaml="http://github.com/fsprojects/FsXaml"
    Title="{Binding Title}"
    Height="200"
    Width="400">
    <Window.DataContext>
        <local:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <Label x:Name="myLabel">Some text</Label>
    </Grid>
</Window>
